get',\n            url: goalsURL,\n            headers: {\n                Authorization: `Token ${token}`,\n            },\n        }).then((res) => {\n            setGoalData(res.data);\n        });\n    }\n\n\n\n\tuseEffect(() => {\n\t\taxios({\n\t\t\tmethod: 'get',\n\t\t\turl: goalsURL,\n\t\t\theaders: {\n\t\t\t\tAuthorization: `Token ${token}`,\n\t\t\t},\n\t\t}).then((res) => {\n\t\t\tsetGoalData(res.data);\n\t\t});\n    }, []);\n\n    const handleSubmit = (event) => {\n        event.preventDefault();\n        axios({\n\t\t\tmethod: 'post',\n\t\t\turl: goalsURL,\n\t\t\theaders: {\n\t\t\t\tAuthorization: `Token ${token}`,\n\t\t\t},\n\t\t\tdata: newGoal,\n\t\t}).then((\n                setShowModal(!showModal),\n                refreshComponent()\n            ))\n        refreshComponent()\n    };\n    \n    const handleDelete = (value) => {\n        axios({\n            method: 'delete',\n            url: `${goalsURL}${value}/`,\n            headers: {\n                Authorization: `Token ${token}`,\n            },\n            data: goalData.key,\n        }).then(refreshComponent())\n        refreshComponent()\n    };\n\n    const handleChange = (event) => {\n        event.preventDefault();\n        setNewGoal({\n            ...newGoal,\n            [event.target.name]: event.target.value,\n        });\n    };\n\n    const handleModal = () => {\n        setShowModal(!showModal)\n    }\n\n    \n    if (!goalData) {\n        return null\n    }\n\n    return (\n\t\t<GoalSectionWrapper>\n\t\t\t<GoalButton onClick={handleModal}>Add New Goal</GoalButton>\n\t\t\t{goalData.map((goal) => {\n\t\t\t\treturn (\n\t\t\t\t\t<GoalWrapper key={goal.id}>\n                        <GoalP>Title: {goal.title}</GoalP>\n                        <GoalP>Priority Level: {goal.priority_level}</GoalP>\n                        <GoalP>Due Date: {goal.due_date}</GoalP>\n                        <GoalP>{goal.description}</GoalP>\n                        <GoalP>Post Date: {goal.set_date}</GoalP>\n\t\t\t\t\t\t<GoalButton\n\t\t\t\t\t\t\tonClick={() => handleDelete(goal.id)}>\n\t\t\t\t\t\t\tDelete\n\t\t\t\t\t\t</GoalButton>\n\t\t\t\t\t</GoalWrapper>\n\t\t\t\t);\n\t\t\t})}\n\t\t\t<GoalsModal showModal={showModal}>\n\t\t\t\t<ModalForm onSubmit={handleSubmit}>\n\t\t\t\t\t<ModalHeader>Goals</ModalHeader>\n\t\t\t\t\t<ModalInput\n\t\t\t\t\t\ttype='text'\n\t\t\t\t\t\tname='title'\n\t\t\t\t\t\tplaceholder='Goal Title'\n\t\t\t\t\t\tvalue={newGoal.title}\n\t\t\t\t\t\tonChange={handleChange}></ModalInput>\n\t\t\t\t\t<ModalLabel htmlFor='title'>Goal Title</ModalLabel>\n\t\t\t\t\t<ModalInput\n\t\t\t\t\t\ttype='text'\n\t\t\t\t\t\tname='description'\n\t\t\t\t\t\tplaceholder='Goal Description'\n\t\t\t\t\t\tvalue={newGoal.description}\n\t\t\t\t\t\tonChange={handleChange}></ModalInput>\n\t\t\t\t\t<ModalLabel htmlFor='description'>\n\t\t\t\t\t\tGoal Description\n\t\t\t\t\t</ModalLabel>\n\t\t\t\t\t<ModalSelect name='priority_level' onChange={handleChange}>\n\t\t\t\t\t\t<option value={newGoal.priority_level}>Low</option>\n\t\t\t\t\t\t<option value={newGoal.priority_level}>Medium</option>\n\t\t\t\t\t\t<option value={newGoal.priority_level}>High</option>\n\t\t\t\t\t</ModalSelect>\n\t\t\t\t\t<ModalLabel htmlFor='priority_level'>\n\t\t\t\t\t\tGoal Priority Level\n\t\t\t\t\t</ModalLabel>\n\t\t\t\t\t<ModalInput\n\t\t\t\t\t\ttype='date'\n\t\t\t\t\t\tname='due_date'\n\t\t\t\t\t\tplaceholder={Date.now()}\n\t\t\t\t\t\tvalue={newGoal.date}\n\t\t\t\t\t\tonChange={handleChange}></ModalInput>\n\t\t\t\t\t<ModalLabel htmlFor='due_date'>Goal Due Date</ModalLabel>\n\t\t\t\t\t<ModalSubmit>Add Goal</ModalSubmit>\n\t\t\t\t</ModalForm>\n\t\t\t</GoalsModal>\n\t\t</GoalSectionWrapper>\n\t);\n};\n\nexport default Goals;","/Users/esquire/sei/projects/human-principle-frontend/src/components/ProfileGoals/ProfileGoalElements.js",[],"/Users/esquire/sei/projects/human-principle-frontend/src/config.js",[],{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","severity":1,"message":